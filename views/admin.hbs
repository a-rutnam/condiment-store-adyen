<h1>Admin: Orders</h1>
<hr/>

<table class="u-full-width">
  <thead>
    <tr>
      <th>id</th>
      <th>user_id</th>
      <th>status</th>
      <th>adyen data</th>
    </tr>
  </thead>
  <tbody>
    {{#each orders}}
    <tr>
      <td>{{ this._id }}</td>
      <td>{{ this.user_id }}</td>
      <td>{{ this.status }}</td>
      <td>
        <pre>
        {{ json this.adyenData }}
      </pre>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
